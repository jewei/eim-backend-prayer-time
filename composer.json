{
    "name": "jewei/prayer-time",
    "description": "Prayer time generation",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Jewei Mak",
            "email": "jewei.mak@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.8",
        "laravel/framework": "^10.28"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "lint": "pint --ansi",
        "phpstan": "phpstan analyse --ansi --memory-limit=-1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
